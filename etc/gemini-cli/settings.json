{
  "context": {
    "fileName": ["AGENTS.md", "CLAUDE.md", "GEMINI.md"]
  },
  "general": {
    "disableAutoUpdate": true,
    "disableUpdateNag": true,
    "checkpointing": {
      "enabled": true
    },
    "previewFeatures": true
  },
  "model": {
    "name": "gemini-3-pro-preview"
  },
  "ide": {
    "enabled": true,
    "hasSeenNudge": true
  },
  "mcpServers": {
    "modular-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@kimuson/modular-mcp",
        "${HOME}/.config/configent/mcp/modular-mcp.json"
      ]
    }
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "tools": {
    "autoAccept": true,
    "shell": {
      "showColor": true
    }
  },
  "ui": {
    "theme": "Ayu",
    "accessibility": {
      "disableLoadingPhrases": true
    }
  },
  "experimental": {
    "useModelRouter": false
  }
}
