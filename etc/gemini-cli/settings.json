{
  "context": {
    "fileName": ["AGENTS.md", "CLAUDE.md", "GEMINI.md"]
  },
  "general": {
    "disableAutoUpdate": true,
    "disableUpdateNag": true,
    "checkpointing": {
      "enabled": true
    }
  },
  "ide": {
    "enabled": true,
    "hasSeenNudge": true
  },
  "mcpServers": {
    "chrome-devtools": {
      "args": ["-y", "chrome-devtools-mcp@latest"],
      "command": "npx"
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest",
        "--api-key",
        "$CONTEXT7_API_KEY"
      ]
    }
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "tools": {
    "autoAccept": true,
    "shell": {
      "showColor": true
    }
  },
  "ui": {
    "theme": "Ayu",
    "accessibility": {
      "disableLoadingPhrases": true
    }
  }
}
