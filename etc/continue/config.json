{
  "models": [
    {
      "title": "gemini-2.0-flash-exp",
      "provider": "gemini",
      "model": "gemini-2.0-flash-exp",
      "apiKey": "[GEMINI_API_KEY]",
      "contextLength": 2097152,
      "completionOptions": {
        "temperature": 0
      }
    },
    {
      "title": "gemini-exp-1206",
      "provider": "gemini",
      "model": "gemini-exp-1206",
      "apiKey": "[GEMINI_API_KEY]",
      "contextLength": 2097152,
      "completionOptions": {
        "temperature": 0
      }
    },
    {
      "title": "o3-mini",
      "provider": "openai",
      "model": "o3-mini",
      "apiKey": "[OPENAI_API_KEY]"
    },
    {
      "title": "claude-3.5-sonnet",
      "provider": "anthropic",
      "model": "claude-3-5-sonnet-latest",
      "apiKey": "[ANTHROPIC_API_KEY]",
      "cacheBehavior": {
        "cacheSystemMessage": true,
        "cacheConversation": true
      },
      "completionOptions": {
        "temperature": 0
      }
    },
    {
      "title": "Ollama: mistral-small:24b",
      "provider": "ollama",
      "model": "mistral-small:latest",
      "apiBase": "http://127.0.0.1:11434",
      "completionOptions": {
        "temperature": 0,
        "keepAlive": 900
      }
    },
    {
      "title": "OpenRouter: R1",
      "provider": "openrouter",
      "model": "deepseek/deepseek-r1",
      "apiKey": "[OPENROUTER_API_KEY]",
      "completionOptions": {
        "temperature": 0
      }
    },
    {
      "title": "Together: R1",
      "provider": "together",
      "model": "deepseek-ai/DeepSeek-R1",
      "apiKey": "[TOGETHER_API_KEY]",
      "completionOptions": {
        "temperature": 0
      }
    },
    {
      "title": "AWS: claude-3.5-sonnet",
      "provider": "bedrock",
      "model": "anthropic.claude-3-5-sonnet-20241022-v2:0",
      "region": "eu-central-1",
      "profile": "bedrock",
      "completionOptions": {
        "temperature": 0
      }
    },
    {
      "title": "Azure: gpt-4o-mini",
      "provider": "azure",
      "model": "gpt-4o-mini",
      "deployment": "gpt-4o-mini",
      "apiBase": "[AZURE_AI_API_BASE]",
      "apiKey": "[AZURE_AI_API_KEY]",
      "completionOptions": {
        "temperature": 0
      }
    }
  ],
  "completionOptions": {
    "stream": true
  },
  "tabAutocompleteModel": {
    "title": "Tab Autocomplete Model",
    "provider": "mistral",
    "model": "codestral-latest",
    "apiBase": "https://codestral.mistral.ai/v1",
    "apiKey": "[CODESTRAL_API_KEY]"
  },
  "embeddingsProvider": {
    "provider": "gemini",
    "model": "models/text-embedding-004",
    "apiKey": "[GEMINI_API_KEY]"
  },
  "reranker": {
    "name": "llm",
    "params": {
      "modelTitle": "gemini-2.0-flash-exp"
    }
  },
  "slashCommands": [
    {
      "name": "share",
      "description": "Export the current chat session to markdown",
      "params": {
        "outputDir": "~/.continue/session-transcripts"
      }
    },
    {
      "name": "cmd",
      "description": "Generate a shell command"
    },
    {
      "name": "commit",
      "description": "Generate a commit message for the current changes"
    },
    {
      "name": "onboard",
      "description": "Familiarize yourself with the codebase"
    },
    {
      "name": "review",
      "description": "Review code and give feedback"
    }

  ],
  "customCommands": [
    {
      "name": "check",
      "description": "Check for mistakes in my code",
      "prompt": "{{{ input }}}\n\nPlease read the highlighted code and check for any mistakes. You should look for the following, and be extremely vigilant:\n- Syntax errors\n- Logic errors\n- Security vulnerabilities\n- Performance issues\n- Anything else that looks wrong\n\nOnce you find an error, please explain it as clearly as possible, but without using extra words. For example, instead of saying 'I think there is a syntax error on line 5', you should say 'Syntax error on line 5'. Give your answer as one bullet point per mistake found."
    },
    {
      "name": "test",
      "description": "Write unit tests for highlighted code",
      "prompt": "Write a comprehensive set of unit tests for the selected code. It should setup, run tests that check for correctness including important edge cases, and teardown. Ensure that the tests are complete and sophisticated. Give the tests just as chat output, don't edit any file."
    }
  ],
  "contextProviders": [
    {
      "name": "file"
    },
    {
      "name": "code"
    },
    {
      "name": "diff"
    },
    {
      "name": "currentFile"
    },
    {
      "name": "terminal"
    },
    {
      "name": "docs"
    },
    {
      "name": "open",
      "params": {
        "onlyPinned": false
      }
    },
    {
      "name": "web",
      "params": {
        "n": 6
      }
    },
    {
      "name": "codebase"
    },
    {
      "name": "folder"
    },
    {
      "name": "search"
    },
    {
      "name": "url"
    },
    {
      "name": "clipboard"
    },
    {
      "name": "tree"
    },
    {
      "name": "problems"
    },
    {
      "name": "debugger",
      "params": {
        "stackDepth": 3
      }
    },
    {
      "name": "repo-map",
      "params": {
        "includeSignatures": true
      }
    },
    {
      "name": "os"
    },
    {
      "name": "google",
      "params": {
        "serperApiKey": "[SERPER_API_KEY]"
      }
    },
    {
      "name": "commit",
      "params": {
        "Depth": 50,
        "LastXCommitsDepth": 10
      }
    }
  ],
  "docs": [
    {
      "title": "Continue",
      "startUrl": "https://docs.continue.dev",
      "rootUrl": "https://docs.continue.dev",
      "maxDepth": 1
    }
  ],
  "tabAutocompleteOptions": {
    "onlyMyCode": false,
    "debounceDelay": 500
  },
  "experimental": {
    "useTools": true,
    "modelContextProtocolServers": [
      {
        "transport": {
          "type": "stdio",
          "command": "uvx",
          "args": [
            "mcp-server-aidd"
          ]
        }
      }
    ],
    "quickActions": [
      {
        "title": "Explain",
        "prompt": "Explain what this code does.",
        "sendToChat": true
      },
      {
        "title": "Fix",
        "prompt": "Fix syntax of this code. If it is already 100% correct, do NOT do anything. Answer only code."
      },
      {
        "title": "Refactor",
        "prompt": "Refactor this code for human readability. Do NOT change the functionality of the code itself."
      },
      {
        "title": "Optimize",
        "prompt": "Optimize this code for performance. Answer only code."
      },
      {
        "title": "Add comment",
        "prompt": "Add comment summarizing what the code does. Do NOT comment every single line. Do NOT change anything about the code itself."
      },
      {
        "title": "Add docstring",
        "prompt": "Write a docstring for this code. Do NOT change anything about the code itself. Answer only code."
      },
      {
        "title": "Add tests",
        "prompt": "Write tests for this code. Do NOT change anything about the code itself. Answer only code."
      }
    ]
  }
}
