{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "duckduckgo-mcp": {
      "command": "uvx",
      "args": [
        "duckduckgo-mcp-server"
      ]
    },
    "mcp-midscene": {
      "command": "npx",
      "args": ["-y", "@midscene/mcp"],
      "env": {
        "MIDSCENE_MODEL_NAME": "gpt-4.1-mini"
      }
    }
  }
}
