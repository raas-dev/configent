{
  "mcpServers": {
    "gateway": {
      "command": "uvx",
      "args": ["agent-mcp-gateway"],
      "env": {
        "GATEWAY_MCP_CONFIG": "${env:GATEWAY_MCP_CONFIG}",
        "GATEWAY_RULES": "${env:GATEWAY_RULES}"
      }
    },
    "utcp": {
      "command": "npx",
      "args": ["-y", "@utcp/code-mode-mcp"],
      "env": {
        "UTCP_CONFIG_FILE": "${env:UTCP_CONFIG_FILE}"
      }
    }
  }
}
