{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false, // false: uses opencode-antigravity-auth plugin
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "prometheus": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "atlas": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "metis": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "momus": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "oracle": {
      "model": "google/antigravity-gemini-3-pro-high"
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash"
    }
  },
  "claude_code": {
    "mcp": false,
    "commands": false,
    "skills": false,
    "agents": false,
    "hooks": false,
    "plugins": false
  },
  "disabled_mcps": [
    "context7",
    "websearch_exa",
    "grep_app"
  ],
  "disabled_skills": [
    "playwright"
  ],
  "experimental": {
    "truncate_all_tool_outputs": true,
    "aggressive_truncation": true,
    "auto_resume": true
  }
}
