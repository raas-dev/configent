{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "sisyphus": {
      "model": "zai-coding-plan/glm-5"
    },
    "hephaestus": {
      "model": "zai-coding-plan/glm-5"
    },
    "prometheus": {
      "model": "zai-coding-plan/glm-5"
    },
    "atlas": {
      "model": "zai-coding-plan/glm-5"
    },
    "sisyphus-junior": {
      "model": "zai-coding-plan/glm-5"
    },
    "oracle": {
      "model": "zai-coding-plan/glm-5"
    },
    "librarian": {
      "model": "zai-coding-plan/glm-5"
    },
    "multimodal-looker": {
      "model": "zai-coding-plan/glm-5"
    },
    "metis": {
      "model": "zai-coding-plan/glm-5"
    },
    "momus": {
      "model": "zai-coding-plan/glm-5"
    },
    "explore": {
      "model": "zai-coding-plan/glm-4.7-flash"
    }
  },
  "claude_code": {
    "mcp": false,
    "commands": false,
    "skills": false,
    "agents": false,
    "hooks": false,
    "plugins": false
  },
  "disabled_mcps": [
    "context7",
    "websearch_exa",
    "grep_app"
  ],
  "disabled_skills": [
    "playwright"
  ],
  "experimental": {
    "truncate_all_tool_outputs": false,
    "aggressive_truncation": false,
    "auto_resume": true
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "tmux": {
    "enabled": false,
    "layout": "main-vertical",
    "main_pane_size": 60,
    "main_pane_min_width": 120,
    "agent_pane_min_width": 40
  }
}
