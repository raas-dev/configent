{
  "env": {
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "1",
    "CLAUDE_CODE_IDE_SKIP_AUTO_INSTALL": "1"
  },
  "permissions": {
    "allow": [
      "Bash",
      "WebSearch",
      "WebFetch"
    ],
    "ask": [
      "Bash(git *)",
      "Bash(rm *)"
    ],
    "defaultMode": "acceptEdits"
  },

  "attribution": {
    "commit": "",
    "pr": ""
  },
  "model": "sonnet",
  "statusLine": {
    "type": "command",
    "command": "bunx -y ccstatusline",
    "padding": 0
  }
}
