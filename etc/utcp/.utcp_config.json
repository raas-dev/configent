{
  "manual_call_templates": [
    {
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "context7": {
            "headers": {
              "CONTEXT7_API_KEY": "${env:CONTEXT7_API_KEY}"
            },
            "transport": "http",
            "url": "https://mcp.context7.com/mcp"
          },
          "tavily": {
            "transport": "http",
            "url": "https://mcp.tavily.com/mcp/?tavilyApiKey=${env:TAVILY_API_KEY}"
          }
        }
      },
      "name": "mcp"
    }
  ]
}
