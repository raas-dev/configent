{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.wordWrap": "on"
  },
  "[shellscript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "activitusbar.activeColour": "#7B9050",
  "breadcrumbs.enabled": false,
  "codeium.enableCodeLens": false,
  "continue.enableTabAutocomplete": false,
  "continue.telemetryEnabled": false,
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "cSpell.suggestionMenuType": "quickFix",
  "cursor.cpp.disabledLanguages": [
    "markdown"
  ],
  "debug.console.fontSize": 13,
  "diffEditor.codeLens": true,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorWidth": 2,
  "editor.fontFamily": "TerminessTTF Nerd Font",
  "editor.fontSize": 17,
  "editor.guides.bracketPairs": "active",
  "editor.hideCursorInOverviewRuler": true,
  "editor.letterSpacing": -0.25,
  "editor.lineHeight": 15.5,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelScrollSensitivity": 0.75,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [
    80,
    100
  ],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "wordWrapColumn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.ejs": "json",
    "*.plist": "plist",
    "*.sh": "shellscript",
    "*.tpl": "json",
    ".aliases": "shellscript"
  },
  "files.hotExit": "onExitAndWindowClose",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.ai.experimental.openai.model": "gpt-4-turbo",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.graph.minimap.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.telemetry.enabled": false,
  "go.useLanguageServer": true,
  "hediet.vscode-drawio.resizeImages": null,
  "javascript.suggest.paths": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "markdown.preview.fontSize": 17,
  "markdown.preview.lineHeight": 1.25,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "powershell.integratedConsole.showOnStartup": false,
  "python.analysis.typeCheckingMode": "basic",
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "rest-client.defaultHeaders": {
    "Content-Type": "application/json",
    "User-Agent": "debug"
  },
  "rest-client.enableTelemetry": false,
  "rest-client.fontFamily": "TerminessTTF Nerd Font",
  "rest-client.fontSize": 17,
  "security.workspace.trust.enabled": false,
  "semgrep.metrics": "off",
  "shellformat.useEditorConfig": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "xterm",
  "terminal.external.osxExec": "Terminal.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "tmux-local",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.profiles.osx": {
    "tmux-local": {
      "args": [
        "new-session",
        "-A",
        "-s",
        "local"
      ],
      "path": "tmux"
    }
  },
  "terminal.integrated.scrollback": 99999,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.tabs.enabled": false,
  "todohighlight.keywords": [
    "AP:"
  ],
  "trivy.fixedOnly": true,
  "trivy.secretScanning": true,
  "typescript.suggest.paths": false,
  "update.showReleaseNotes": false,
  "window.closeWhenEmpty": true,
  "window.customTitleBarVisibility": "windowed",
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "off",
  "window.restoreFullscreen": true,
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#353535",
    "terminal.ansiBlue": "#68A8E4",
    "terminal.ansiBrightBlack": "#535353",
    "terminal.ansiBrightBlue": "#2C78BF",
    "terminal.ansiBrightCyan": "#0AAEB3",
    "terminal.ansiBrightGreen": "#B9C949",
    "terminal.ansiBrightMagenta": "#6E29AD",
    "terminal.ansiBrightRed": "#EF2F27",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#F5AE2E",
    "terminal.ansiCyan": "#2BE4D0",
    "terminal.ansiGreen": "#AFD700",
    "terminal.ansiMagenta": "#AA54F9",
    "terminal.ansiRed": "#F75341",
    "terminal.ansiWhite": "#F2F2F2",
    "terminal.ansiYellow": "#FED06E",
    "terminal.background": "#090A14",
    "terminal.foreground": "#C9C9C9",
    "terminalCursor.background": "#353535",
    "terminalCursor.foreground": "#A3A3A3"
  },
  "workbench.colorTheme": "Ayu Dark Bordered",
  "workbench.editor.defaultBinaryEditor": "default",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "ayu",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Ayu Dark Bordered",
  "workbench.preferredHighContrastColorTheme": "Ayu Dark Bordered",
  "workbench.preferredLightColorTheme": "Ayu Dark Bordered",
  "workbench.startupEditor": "newUntitledFile",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false
}
