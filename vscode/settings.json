{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.wordWrap": "on"
  },
  "[shellscript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "activitusbar.activeColour": "#7B9050",
  "activitusbar.toggleSidebar": false,
  "bicep.enableSurveys": false,
  "breadcrumbs.enabled": false,
  "codeium.aggressiveShutdown": true,
  "codeium.enableCodeLens": false,
  "codeium.enableInComments": false,
  "continue.enableTabAutocomplete": true,
  "continue.telemetryEnabled": false,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "elixir",
    "erlang",
    "git-commit",
    "go",
    "graphql",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "jupyter",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "scminput",
    "swift",
    "text",
    "typescript",
    "typescriptreact",
    "vue",
    "yaml",
    "yml"
  ],
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "cSpell.suggestionMenuType": "quickFix",
  "cursor.cpp.disabledLanguages": [
    "markdown"
  ],
  "debug.console.fontSize": 13,
  "diffEditor.codeLens": true,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorWidth": 2,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "TerminessTTF Nerd Font",
  "editor.fontSize": 17,
  "editor.guides.bracketPairs": "active",
  "editor.hideCursorInOverviewRuler": true,
  "editor.letterSpacing": -0.25,
  "editor.lineHeight": 15.5,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [
    80,
    100
  ],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "wordWrapColumn",
  "excalidraw.theme": "dark",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.ejs": "json",
    "*.plist": "plist",
    "*.sh": "shellscript",
    "*.tpl": "json",
    ".aliases": "shellscript"
  },
  "files.hotExit": "onExitAndWindowClose",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.ai.experimental.openai.model": "gpt-4o",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.graph.minimap.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.telemetry.enabled": false,
  "go.survey.prompt": false,
  "go.useLanguageServer": true,
  "hediet.vscode-drawio.resizeImages": null,
  "javascript.suggest.paths": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "markdown.preview.fontSize": 17,
  "markdown.preview.lineHeight": 1.25,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "powershell.integratedConsole.showOnStartup": false,
  "problems.visibility": false,
  "python.analysis.typeCheckingMode": "basic",
  "redhat.telemetry.enabled": false,
  "rest-client.defaultHeaders": {
    "Content-Type": "application/json",
    "User-Agent": "debug"
  },
  "rest-client.enableTelemetry": false,
  "rest-client.fontFamily": "TerminessTTF Nerd Font",
  "rest-client.fontSize": 17,
  "security.workspace.trust.enabled": false,
  "semgrep.metrics": "off",
  "shellformat.useEditorConfig": true,
  "simple-lines-count.format": "Lns: {t} ({e} empty)",
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "xterm",
  "terminal.external.osxExec": "Terminal.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "tmux-local",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.mouseWheelZoom": true,
  "terminal.integrated.profiles.osx": {
    "tmux-local": {
      "args": [
        "new-session",
        "-A",
        "-s",
        "local"
      ],
      "path": "tmux"
    }
  },
  "terminal.integrated.scrollback": 99999,
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.tabs.enabled": false,
  "todohighlight.keywords": [
    "AP:"
  ],
  "trivy.fixedOnly": true,
  "trivy.secretScanning": true,
  "typescript.suggest.paths": false,
  "typescript.surveys.enabled": false,
  "update.showReleaseNotes": false,
  "window.closeWhenEmpty": true,
  "window.confirmSaveUntitledWorkspace": false,
  "window.customTitleBarVisibility": "windowed",
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "off",
  "window.restoreFullscreen": true,
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#353535",
    "terminal.ansiBlue": "#1398B9",
    "terminal.ansiBrightBlack": "#535353",
    "terminal.ansiBrightBlue": "#5FDAFF",
    "terminal.ansiBrightCyan": "#0AAEB3",
    "terminal.ansiBrightGreen": "#B9CA49",
    "terminal.ansiBrightMagenta": "#FF00F6",
    "terminal.ansiBrightRed": "#EF2F27",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FED06E",
    "terminal.ansiCyan": "#2BE4D0",
    "terminal.ansiGreen": "#AFD700",
    "terminal.ansiMagenta": "#AA54F9",
    "terminal.ansiRed": "#F75341",
    "terminal.ansiWhite": "#F2F2F2",
    "terminal.ansiYellow": "#FBB829",
    "terminal.background": "#090A14",
    "terminal.foreground": "#D0D0D0",
    "terminalCursor.background": "#353535",
    "terminalCursor.foreground": "#B300AD"
  },
  "workbench.colorTheme": "Ayu Dark Bordered",
  "workbench.editor.defaultBinaryEditor": "default",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "ayu",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Ayu Dark Bordered",
  "workbench.preferredHighContrastColorTheme": "Ayu Dark Bordered",
  "workbench.preferredLightColorTheme": "Ayu Dark Bordered",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false
}
